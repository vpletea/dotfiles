{
  "theme": "Catppuccin Mocha",
  "telemetry": {
    "diagnostics": false,
    "metrics": false
  },
  "vim_mode": false,
  "tab_size": 2,
  "preferred_line_length": 80,
  "soft_wrap": "preferred_line_length",
  "format_on_save": "on",
  "formatter": "language_server",
  "autosave": {
    "after_delay": {
      "milliseconds": 1000
    }
  },
  "terminal": {
    "font_family": "JetBrainsMono Nerd Font Mono",
    "font_size": 16,
    "blinking": "on",
    "shell": {
      "program": "zsh"
    }
  },
  "language_overrides": {
    "Nix": {
      "format_on_save": "off"
    }
  },
  "show_copilot_suggestions": true,
  "projects_online_by_default": true,
  "languages": {
    "Nix": {
      "formatter": {
        "external": {
          "command": "nixpkgs-fmt"
        }
      }
    }
  },
  "buffer_font_size": 16,
  "buffer_font_family": "JetBrainsMono Nerd Font Mono",
  "base_keymap": "VSCode",
  "collaboration": {
    "status_badge": {
      "enabled": false
    }
  },
  "extensions": [
    "catppuccin.catppuccin",
    "zed.copilot",
    "zed.terraform",
    "zed.toml",
    "zed.nix",
    "zed.markdown",
    "zed.yaml",
    "zed.json",
    "zed.docker",
    "zed.bash",
    "zed.git",
    "github.copilot"
  ]
}
